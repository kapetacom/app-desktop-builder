    <script>
        window.Kapeta = {
            paths: [],
            config: {},
            resourceTypes: {},
            blockTypes: {},
            languageTargets: {},
            deploymentTargets: {},
            setPluginPaths(paths) { //Method gets called by local-cluster-service
                this.paths = paths;
            }
        };
    </script>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');

        body,
        html {
            background-color: #F5F1EE;
        }
    </style>

    <script>
        if (location.search && location.search.length > 1) {
            const keyValuePairs = new URLSearchParams(location.search);

            for (const [key, value] of keyValuePairs.entries()) {
                window.Kapeta.config[key] = value;
            }
            const clusterBase = keyValuePairs.get('cluster_service');
            const safeDomains = [
                'https://*.googletagmanager.com',
                'https://pendo-eu-static-5224383087443968.storage.googleapis.com',
                'https://*.pendo.io',
                'https://*.heapanalytics.com',
                'http://cdn.heapanalytics.com',
                'https://*.heap-api.com',
                'https://cdn.segment.com',
                'https://cdn.jsdelivr.net'
            ];

            if (clusterBase && (
                clusterBase.startsWith('http://127.0.0.1:') ||
                clusterBase.startsWith('http://localhost:') ||
                clusterBase.startsWith('http://::1:')
            )) {
                safeDomains.push(clusterBase);
            }

            document.write(`
            <meta
                    http-equiv="Content-Security-Policy"
                    content="script-src 'unsafe-eval' 'self' ${safeDomains.join(' ')};worker-src blob: 'self' ${safeDomains.join(' ')}"
            />`
            );
        }
    </script>
    <meta charset="utf-8" />

    <title>Kapeta</title>
