{
    "compilerOptions": {
        "incremental": true,
        "target": "es2022",
        "module": "ESNext",
        "jsx": "react-jsx",
        "strict": false,
        "sourceMap": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strictNullChecks": true,
        "useDefineForClassFields": true,
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "allowJs": true
    }
}
